{
    "name": "globi-data",
    "version": "0.0.8",
    "description": "Explore species interactions: who eats who, or who is a host for whom. Data provided by EOL's GloBI (Global Biotic Interactions).",
    "main": "globi-data.js",
    "directories": {
        "example": "example"
    },
    "dependencies": {

    },
    "devDependencies": {
        "tape": "~1.0.4",
        "testling": "~1.5.3"
    },
    "testling": {
        "files": "test/*.js",
        "browsers": [ "ie8", "ie9", "ff/13", "chrome/20" ]
    },

    "scripts": {
        "test": "testling",
        "prepublish": "browserify . --standalone globi-data -o globi-data-dist.js"
    },

    "repository": {
        "type": "git",
        "url": "git://github.com/jhpoelen/eol-globi-data-js.git"
    },
    "keywords": [
        "eol",
        "encyclopedia of life",
        "sglobal biotic interactions",
        "species",
        "interactions",
        "bioinformatics",
        "browser",
        "ecology",
        "food web",
        "foodweb",
        "biodiversity",
        "big data",
        "small data"
    ],
    "author": "Jorrit Poelen",
    "license": "BSD",
    "readmeFilename": "README.md",
    "bugs": {
        "url": "https://github.com/jhpoelen/eol-globi-data-js/issues"
    }
}
