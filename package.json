{
    "name": "globi-data",
    "version": "0.0.4",
    "description": "library to access EOL's Global Biotic Interactions Data http://github.com/jhpoelen/eol-globi-data",
    "main": "globi-data.js",
    "directories": {
        "example": "example"
    },
    "dependencies": {
        "d3": "~3.2.5"
    },
    "devDependencies": {
        "tape": "~1.0.4"
    },
    "testling": {
